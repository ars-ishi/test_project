<p id="notice"><%= notice %></p>

<h1>Squires</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @squires.each do |squire| %>
      <tr>
        <td><%= squire.title %></td>
        <td><%= squire.description %></td>
        <td><%= link_to 'Show', squire %></td>
        <td><%= link_to 'Edit', edit_squire_path(squire) %></td>
        <td><%= link_to 'Destroy', squire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Squire', new_squire_path %>
